{
  "name": "mini-skyway",
  "version": "0.0.1",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "test": "npm run type && jest --forceExit",
    "type": "tsc --noEmit --project ./tsconfig.json",
    "start": "ls && ts-node --project ./tsconfig.json ./main.ts"
  },
  "dependencies": {
    "express": "^4.17.1",
    "rxjs": "^6.5.5",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^14.0.1",
    "@types/socket.io": "^2.1.4",
    "jest": "^26.0.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "tslib": "^1.12.0",
    "typescript": "^3.9.2"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
